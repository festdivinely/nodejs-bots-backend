{
    "version": 2,
    "functions": {
        "app.js": {
            "includeFiles": "{routes/**,config/**,middleware/**,utility/**}",
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
}