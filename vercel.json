{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "rewrites": [
        {
            "source": "/api/auth/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/api/bots/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/api",
            "destination": "/api/index.js"
        }
    ]
}