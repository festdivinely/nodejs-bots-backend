{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "functions": {
        "api/**/*.js": {
            "runtime": "@vercel/node@2.0.0",
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/",
            "destination": "/api/index.js"
        }
    ]
}